local function local_rules(ast)
   local locals = ast:select(notrecursive)
   local phrase = \"\"
   for i = 1, #locals do
      phrase = phrase..\"    \"..locals[i]:flatten()
   end
   return phrase
end

local function cursive_rules(ast)
   local cursives = ast:select(isrecursive)
   local phrase  = \"\"
   for i = 1, #cursives do
      phrase = phrase..\"  \"..cursives[i]:flatten()
   end
   return phrase
end

