codeblock_240 -> leaf_268
leaf_268  [color=Gray,shape=rectangle,fontname=Inconsolata,label="function Path.parentDir(path, dir)
  if not path.isDir then
    return nil
  end

  for i = #path, 1, -1 do
    if path[i] == dir then
      local path_phrase = \"\"
      for j = 1, i do
        path_phrase = path_phrase .. path[j]
      end
      return new(path_phrase .. \"/\")
    end
  end

  return nil
end

"]
