function Inv.write_footer(inverter)
    return \"#/\" .. inverter.lang .. \"\\n\"
end

function Inv.filter(inverter, line)
    if isBlank(line) then
        return \"\"
    else 
        return line:gsub(\"\\t\", inverter.tab_to_space):gsub(\"\\r\", \"\")
    end
end

